<!DOCTYPE html>
<html>
<head>
  <title>Dealerapp</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

	

	<div class="container">
		<% flash.each do |key, value| %>
        	<div class="alert alert-<%= key %>"><%= value %></div>
      	<% end %>
		<%= yield %>
	</div>

	<%= debug(params) if Rails.env.development? %>

</body>
</html>
